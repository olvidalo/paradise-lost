{
  "name": "paradise-lost",
  "version": "1.0.0",
  "description": "A Map of the Moralized Geography of Paradise Lost",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c -w --environment NODE_ENV:dev,PORT:8080",
    "build:dev": "rollup -c -w --environment NODE_ENV:dev",
    "build:prod": "npm run preprocess && npm run validate && rollup -c --environment NODE_ENV:prod",
    "deploy": "npm run build:prod && rsync -avz dist/ aldebaran.uberspace.de:html/paradise-lost",
    "validate": "node scripts/validate-data.js",
    "preprocess": "node scripts/preprocess-data.js"
  },
  "author": "Marcel Schaeben",
  "license": "GPLv3",
  "dependencies": {
    "Leaflet.vector-markers": "github:ajf2/Leaflet.vector-markers#90ef9b81a87611fc1ef20eac8c4f5581287a4d7f",
    "animate.css": "^3.7.2",
    "d3": "^7.6.1",
    "leaflet": "^1.5.1",
    "leaflet-providers": "^1.9.0",
    "leaflet.markercluster": "^1.4.1",
    "marked": "^0.3.19",
    "strip-bom": "^3.0.0",
    "typeface-playfair-display": "0.0.54",
    "vue": "^2.6.10",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.3",
    "vuetify": "^1.5.18",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
