    "leaflet.sync": "^0.2.4",
    "postcss": "^7.0.18",
    "poststylus": "^1.0.0",
    "rollup": "git+https://github.com/passcod/rollup.git",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-dsv": "^1.2.0",
